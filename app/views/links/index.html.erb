<h1>All Links</h1>
<%= form_for @link do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title  %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <%= f.submit %>
<% end %>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
  </tr>
  <% @user.links.each do |link| %>
  <tr>
    <td><%= link.title %>
    <td><%= link.url %>
  </tr>
  <% end %>
</table>
